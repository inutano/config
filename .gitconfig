[user]
  name  = Tazro Inutano Ohta
  email = inutano@gmail.com

[github]
  user = inutano

[core]
  editor = nano
  autocrlf = input
  safecrlf = true

[web]
  browser = safari

[color]
  ui = auto

[color "branch"]
  current = yellow bold
  local   = green bold
  remote  = cyan bold

[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old  = red bold
  new  = green bold
  whitespace = red reverse

[color "status"]
  added     = green bold
  changed   = yellow bold
  untracked = red bold

[alias]
  cmt = commit -m
  st = status
  hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
  ls = ls-files

[hub]
  protocol = https
